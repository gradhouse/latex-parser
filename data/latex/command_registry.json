    "section": {
      "command": "\\section",
      "description": "Start a new section.",
      "syntax": "\\section[short_title]{title}",
      "arguments": [
        {
          "name": "short_title",
          "type": "optional",
          "form": "[]",
          "fallback_to": "#2",
          "note": "Used in TOC and headers. Uses the second argument 'title' if omitted"
        },
        {
          "name": "title",
          "type": "required",
          "form": "{}"
        }
      ],
      "implementation": null,
      "examples": [
        "\\section[Intro]{Introduction}"
      ],
      "notes": [],
